<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_init.xml"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.engway.ui.init.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_accountActivity"
            app:destination="@id/accountActivity" />
    </fragment>
    <activity
        android:id="@+id/accountActivity"
        android:name="com.example.engway.ui.account.AccountActivity"
        android:label="activity_account"
        tools:layout="@layout/activity_account" />


</navigation>